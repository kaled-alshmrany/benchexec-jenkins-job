<?xml version="1.0" ?>
<!DOCTYPE table PUBLIC "+//IDN sosy-lab.org//DTD BenchExec table 1.10//EN" "https://www.sosy-lab.org/benchexec/table-1.10.dtd">
<table>

  <result>
    <!-- <column> can be used here to override the global set of columns.  -->
    <column title="status"/>
    <column title="cputime" displayTitle="CPU Time"/>
    <column title="memory" displayTitle="Memory" sourceUnit="B" displayUnit="MB"/>
  </result>

  <union>
    <result id="ReachSafety-Arrays" filename="./ReachSafety-Arrays/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-BitVectors" filename="./ReachSafety-BitVectors/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-ControlFlow" filename="./ReachSafety-ControlFlow/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-Floats" filename="./ReachSafety-Floats/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-Heap" filename="./ReachSafety-Heap/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-Loops" filename="./ReachSafety-Loops/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-Recursive" filename="./ReachSafety-Recursive/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-ECA" filename="./ReachSafety-ECA/output-tool.logtool-*.xml"/>
    <result id="ReachSafety-Sequentialized" filename="./ReachSafety-Sequentialized/output-tool.logtool-*.xml"/>
    
    <result id="SoftwareSystems-DeviceDriversLinux64-ReachSafety" filename="./SoftwareSystems-DeviceDriversLinux64-ReachSafety/output-tool.logtool-*.xml"/>
    <result id="SoftwareSystems-BusyBox-MemSafety" filename="./SoftwareSystems-BusyBox-MemSafety/output-tool.logtool-*.xml"/>
    
    <result id="Termination-MainHeap" filename="./Termination-MainHeap/output-tool.logtool-*.xml"/>

    <result id="SoftwareSystems-SQLite-MemSafety" filename="./SoftwareSystems-SQLite-MemSafety/output-tool.logtool-*.xml"/>
  </union>

</table>
