<?xml version="1.0" ?>
<!DOCTYPE table PUBLIC "+//IDN sosy-lab.org//DTD BenchExec table 1.10//EN" "https://www.sosy-lab.org/benchexec/table-1.10.dtd">
<table>

  <result>
    <!-- <column> can be used here to override the global set of columns.  -->
    <column title="status"/>
    <column title="cputime" displayTitle="CPU Time"/>
    <column title="branches_covered" displayTitle="Branchs Covered (%)"/>
    <column title="branches_total" displayTitle="Branchs Total"/>
    <column title="memory" displayTitle="Memory" sourceUnit="B" displayUnit="MB"/>
  </result>

  <union>
    <result id="ReachSafety-Arrays" filename="./ReachSafety-Arrays/*.xml"/>
    <result id="ReachSafety-BitVectors" filename="./ReachSafety-BitVectors/*.xml"/>
    <result id="ReachSafety-ControlFlow" filename="./ReachSafety-ControlFlow/*.xml"/>
    <result id="ReachSafety-Floats" filename="./ReachSafety-Floats/*.xml"/>
    <result id="ReachSafety-Heap" filename="./ReachSafety-Heap/*.xml"/>
    <result id="ReachSafety-Loops" filename="./ReachSafety-Loops/*.xml"/>
    <result id="ReachSafety-Recursive" filename="./ReachSafety-Recursive/*.xml"/>
    <result id="ReachSafety-Sequentialized" filename="./ReachSafety-Sequentialized/*.xml"/>
    
    <result id="SoftwareSystems-DeviceDriversLinux64-ReachSafety" filename="./SoftwareSystems-DeviceDriversLinux64-ReachSafety/*.xml"/>
    <result id="SoftwareSystems-BusyBox-MemSafety" filename="./SoftwareSystems-BusyBox-MemSafety/*.xml"/>
    
    <result id="Termination-MainHeap" filename="./Termination-MainHeap/*.xml"/>

    <result id="SoftwareSystems-SQLite-MemSafety" filename="./SoftwareSystems-SQLite-MemSafety/*.xml"/>
  </union>

</table>
